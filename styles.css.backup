}

/* Service Details Section */
.service-details-section {
    padding: 6rem 0;
    background: var(--white);
}

.service-content-grid {
    display: grid;
    grid-template-columns: 1fr 350px;
    gap: 4rem;
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 2rem;
}

.service-main-content {
